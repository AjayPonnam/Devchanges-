<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>CASE001_Engineering_Req_Numeric</fullName>
        <active>true</active>
        <description>Engineering Req Number field should be Numeric</description>
        <errorConditionFormula>AND(NOT( ISBLANK( EngineeringReqNumber__c ) ), NOT(ISNUMBER( EngineeringReqNumber__c ) ) )</errorConditionFormula>
        <errorDisplayField>EngineeringReqNumber__c</errorDisplayField>
        <errorMessage>Engineering Req Field must be numeric [CASE001]</errorMessage>
    </validationRules>
</CustomObject>
